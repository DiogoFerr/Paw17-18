extends layout
block content
    h2 Zona de Triagem  
    h3 Pacientes restantes: #{pacientes.length}
    table
        tr
            th NUS
            th Paciente
        each paciente in pacientes
            tr
                td #{paciente.NUS} 
                td
                    a(href="/triagem/perfilPaciente/" + paciente.NUS)  #{paciente.nome}
    button(onClick = "location.href='/triagem/pacientesAtendidos'") Pacientes Atendidos
    button(onClick = "location.href='/logout'") Logout